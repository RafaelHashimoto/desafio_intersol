<% if @transaction.errors.any? %>
  <%byebug%>
  toastr.error("<%=@transaction.errors.first[1]%>");
  $("<%=id%>").html("<%= escape_javascript(render action) %>")
  $('select').material_select();  
<% else %>
  toastr.success("Sucesso");
  $("<%=partial_id%>").html("<%= escape_javascript(render partial) %>")
  $("<%=id%>").modal("close")
<% end %>